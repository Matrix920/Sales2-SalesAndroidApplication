            android:textAlignment="textEnd" />

        <TextView
            android:id="@+id/txtLebanon"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
